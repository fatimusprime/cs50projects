#include <stdio.h>
#include <cs50.h>
#include <string.h>
#include <ctype.h>

int main(void)
{
    string a = GetString();
    string alpha = "abcdefghijklmnopqrstuvwxyz";
    int m = 0;
    //int lmin = 97;
   // int lmax = 122;
    
    printf("%s\n", alpha);

    
    for (int i = 0; i < strlen(a); i++)
        {
           if (isupper(a[i]))
           {
                if (a[i] >= 65 && a[i] <= 90)
                {
                    printf("Its upper\n");
                    m = ((a[i] - 65 + 1) % 26) + 65;
                    printf("New:%c\n", m);
                }
           }
           else
           {
                if (islower(a[i]))
                {
                    m = ((a[i] - 97 + 1) % 26) + 97;
                    printf("Its lower\n");
                    printf("New:%c\n", m);
                }
           }
            printf("%i\n", a[i]);
        }
}
